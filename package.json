{
	"name": "util.obj-cycle",
	"version": "0.0.1",
	"debug": false,
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "Monkey patched additional JSON functionality to handle cyclical objects",
	"main": "./index.js",
	"types": "./index.d.ts",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.obj-cycle"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.obj-cycle/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"devDependencies": {
		"@babel/core": "^7.3.3",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/runtime": "^7.3.1",
		"@types/fs-extra": "5.0.5",
		"@types/jest": "^24.0.5",
		"@types/node": "^11.9.4",
		"babel-jest": "^24.1.0",
		"babel-minify": "^0.5.0",
		"coveralls": "^3.0.2",
		"jest": "^24.1.0",
		"prettier": "^1.16.4",
		"tslint": "^5.12.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.3.3",
		"util.pkgwrap": "^0.0.94"
	},
	"pkgwrap": {
		"cleanup": [
			"docs",
			"logs",
			"index.d.ts",
			"index.js",
			"src/cycle.min.js"
		]
	},
	"jest": {
		"collectCoverage": true,
		"coveragePathIgnorePatterns": [
			"<rootDir>/node_modules"
		],
		"moduleFileExtensions": [
			"js",
			"jsx",
			"ts",
			"tsx",
			"json"
		],
		"notify": false,
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules"
		],
		"verbose": true
	},
	"scripts": {
		"all": "yarn install && yarn run prettier && yarn run build && yarn test",
		"build": "yarn run clean && yarn run lint && yarn run minify && pkgwrap --build",
		"clean": "pkgwrap --clean",
		"coverage": "pkgwrap --coverage --jest",
		"lint": "pkgwrap --lint",
		"minify": "minify ./src/cycle.js --out-file ./src/cycle.min.js",
		"prepublishOnly": "yarn run all",
		"prettier": "prettier --write \"**/*.{ts,tsx,js,json,yaml,yml,css}\"",
		"report": "pkgwrap --reporting",
		"test": "jest"
	}
}
